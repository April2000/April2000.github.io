(function(a){'undefined'==typeof $document&&($document=a(document)),a.fn.spacedClickOut=function(b,c){var d='.spacedClickOut_'+Math.floor(9000*Math.random()+1e3);return setTimeout(a.proxy(function(){$document.on('mousedown'+d,a.proxy(function(f){a(f.target).is(this)||0!==this.has(f.target).length||c&&(a(f.target).is(c)||!(1>a(f.target).closest(c).length))||($document.off('mousedown'+d),f.delegateTarget=this.get(),'function'==typeof b&&b.call(this,f))},this))},this),50),'mousedown'+d}})(jQuery);