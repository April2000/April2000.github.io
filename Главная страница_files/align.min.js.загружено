(function(a){a(jQuery)})(function(a){a.FE.PLUGINS.align=function(b){return{apply:function(g){b.selection.save(),b.html.wrap(!0,!0,!0,!0),b.selection.restore();for(var h=b.selection.blocks(),j=0;j<h.length;j++)b.helpers.getAlignment(a(h[j].parentNode))==g?a(h[j]).css('text-align','').removeClass('fr-temp-div'):a(h[j]).css('text-align',g).removeClass('fr-temp-div'),''===a(h[j]).attr('class')&&a(h[j]).removeAttr('class'),''===a(h[j]).attr('style')&&a(h[j]).removeAttr('style');b.selection.save(),b.html.unwrap(),b.selection.restore()},refresh:function(g){var h=b.selection.blocks();if(h.length){var j=b.helpers.getAlignment(a(h[0]));g.find('> *:first').replaceWith(b.icon.create('align-'+j))}},refreshOnShow:function(g,h){var j=b.selection.blocks();if(j.length){var k=b.helpers.getAlignment(a(j[0]));h.find('a.fr-command[data-param1="'+k+'"]').addClass('fr-active').attr('aria-selected',!0)}}}},a.FE.DefineIcon('align',{NAME:'align-left'}),a.FE.DefineIcon('align-left',{NAME:'align-left'}),a.FE.DefineIcon('align-right',{NAME:'align-right'}),a.FE.DefineIcon('align-center',{NAME:'align-center'}),a.FE.DefineIcon('align-justify',{NAME:'align-justify'}),a.FE.RegisterCommand('align',{type:'dropdown',title:'Align',options:{left:'Align Left',center:'Align Center',right:'Align Right',justify:'Align Justify'},html:function(){var b='<ul class="fr-dropdown-list" role="presentation">',d=a.FE.COMMANDS.align.options;for(var e in d)d.hasOwnProperty(e)&&(b+='<li role="presentation"><a class="fr-command fr-title" tabIndex="-1" role="option" data-cmd="align" data-param1="'+e+'" title="'+this.language.translate(d[e])+'">'+this.icon.create('align-'+e)+'<span class="fr-sr-only">'+this.language.translate(d[e])+'</span></a></li>');return b+='</ul>',b},callback:function(b,d){this.align.apply(d)},refresh:function(b){this.align.refresh(b)},refreshOnShow:function(b,d){this.align.refreshOnShow(b,d)},plugin:'align'})});