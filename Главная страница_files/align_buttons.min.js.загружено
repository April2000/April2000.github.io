(function(a){a.FE.RegisterCommand('alignLeft',{undo:!0,title:'Align Left',icon:'align-left',callback:function(){this.align.apply('left')},refresh:function(b){var c=this.selection.blocks();c.length&&b.toggleClass('fr-active','left'===this.helpers.getAlignment(a(c[0])))},plugin:'align'}),a.FE.RegisterCommand('alignRight',{undo:!0,title:'Align Right',icon:'align-right',callback:function(){this.align.apply('right')},refresh:function(b){var c=this.selection.blocks();c.length&&b.toggleClass('fr-active','right'===this.helpers.getAlignment(a(c[0])))},plugin:'align'}),a.FE.RegisterCommand('alignCenter',{undo:!0,title:'Align Center',icon:'align-center',callback:function(){this.align.apply('center')},refresh:function(b){var c=this.selection.blocks();c.length&&b.toggleClass('fr-active','center'===this.helpers.getAlignment(a(c[0])))},plugin:'align'}),a.FE.RegisterCommand('justify',{undo:!0,title:'Align Justify',icon:'align-justify',callback:function(){this.align.apply('justify')},refresh:function(b){var c=this.selection.blocks();c.length&&b.toggleClass('fr-active','justify'===this.helpers.getAlignment(a(c[0])))},plugin:'align'})})(jQuery);