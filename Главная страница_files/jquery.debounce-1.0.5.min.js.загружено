(function(a){a.extend({debounce:function(b,c,d,e){3==arguments.length&&'boolean'!=typeof d&&(e=d,d=!1);var f;return c||(c=0),function(){var g=arguments;e=e||this,d&&!f&&b.apply(e,g),clearTimeout(f),f=setTimeout(function(){d||b.apply(e,g),f=null},c)}},throttle:function(b,c,d){var e,f,g;return function(){f=arguments,g=!0,d=d||this,e||function(){g?(b.apply(d,f),g=!1,e=setTimeout(arguments.callee,c)):e=null}()}}})})(jQuery);